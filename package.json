{
  "name": "uniwars-mobile-client",
  "version": "0.1",
  "scripts": {
    "start": "expo start --lan --no-dev",
    "dev": "expo start --lan",
    "test": "node_modules/.bin/jest",
    "docs": "./node_modules/.bin/esdoc && open ./docs/index.html"
  },
  "main": "node_modules/expo/AppEntry.js",
  "jest": {
    "preset": "jest-expo"
  },
  "esdoc": {
    "source": "./src",
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      },
      {
        "name": "esdoc-react-plugin"
      },
      {
        "name": "esdoc-ecmascript-proposal-plugin",
        "option": {
          "all": true
        }
      },
      {
        "name": "esdoc-jsx-plugin",
        "option": {
          "enable": true
        }
      }
    ]
  },
  "resolutions": {
    "mobx": "^4.3.0",
    "mobx-react": "^5.1.2"
  },
  "private": true,
  "dependencies": {
    "autobind-decorator": "^2.1.0",
    "awesome-phonenumber": "^2.2.5",
    "axios": "^0.18.0",
    "expo": "^30.0.0",
    "lodash": "^4.17.10",
    "mobx": "^4.3.0",
    "mobx-react": "^5.1.2",
    "prop-types": "^15.6.0",
    "react": "16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-30.0.0.tar.gz",
    "react-native-autocomplete-input": "robertosero/react-native-autocomplete-input",
    "react-native-bouncing-preloader": "^1.0.1",
    "react-native-button": "^2.3.0",
    "react-native-cards": "^1.1.4",
    "react-native-carousel-control": "^2.0.1",
    "react-native-circular-progress": "^1.0.0",
    "react-native-country-picker-modal": "^0.5.1",
    "react-native-debug-stylesheet": "^0.1.1",
    "react-native-easy-toast": "^1.1.0",
    "react-native-elements": "^1.0.0-beta2",
    "react-native-gifted-chat": "^0.3.0",
    "react-native-router-flux": "^4.0.0-beta.28",
    "react-native-sideswipe": "^1.4.2",
    "react-native-snap-carousel": "^3.7.2",
    "react-native-swiper": "robertosero/react-native-swiper",
    "react-native-table-component": "^1.1.3",
    "react-native-user-avatar": "^1.0.3",
    "socket.io-client": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-stage-1": "^6.24.1",
    "documentation": "^8.1.2",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-react-plugin": "^1.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "jest-expo": "^30.0.0",
    "react-docgen": "^2.21.0"
  }
}
